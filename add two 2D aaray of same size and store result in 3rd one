#include <stdio.h>
int main(void){
    int r=2,c=3,i,j;
    int A[2][3] = {{1,2,3},{4,5,6}};
    int B[2][3] = {{6,5,4},{3,2,1}};
    int C[2][3];
    for(i=0;i<r;i++) for(j=0;j<c;j++) C[i][j]=A[i][j]+B[i][j];
    printf("Sum:\n");
    for(i=0;i<r;i++){ for(j=0;j<c;j++) printf("%d ",C[i][j]); printf("\n"); }
    return 0;
}
