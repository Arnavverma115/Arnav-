#include <stdio.h>
int main(void){
    int a[10] = {1,5,3,5,2,5,7,8,5,9}, n=10, val=5;
    int idx=-1;
    for(int i=0;i<n;i++) if(a[i]==val){ idx=i; break; }
    if(idx==-1) { printf("Not found\n"); return 0; }
    for(int i=idx;i<n-1;i++) a[i]=a[i+1];
    n--;
    for(int i=0;i<n;i++) printf("%d ", a[i]);
    printf("\n");
    return 0;
}
