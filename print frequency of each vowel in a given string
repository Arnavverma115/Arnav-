#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(void){
    char s[500]; int freq[5]={0}; // a e i o u
    fgets(s,sizeof s,stdin);
    for(int i=0;s[i];i++){
        char c = tolower((unsigned char)s[i]);
        if(c=='a') freq[0]++;
        else if(c=='e') freq[1]++;
        else if(c=='i') freq[2]++;
        else if(c=='o') freq[3]++;
        else if(c=='u') freq[4]++;
    }
    printf("a=%d e=%d i=%d o=%d u=%d\n", freq[0],freq[1],freq[2],freq[3],freq[4]);
    return 0;
}
