#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(void){
    char s[200];
    fgets(s,sizeof s,stdin); s[strcspn(s,"\n")]=0;
    int i=0,j=strlen(s)-1, ok=1;
    while(i<j){
        while(i<j && !isalnum((unsigned char)s[i])) i++;
        while(i<j && !isalnum((unsigned char)s[j])) j--;
        if(tolower((unsigned char)s[i]) != tolower((unsigned char)s[j])) { ok=0; break; }
        i++; j--;
    }
    printf("%s\n", ok ? "Palindrome" : "Not a palindrome");
    return 0;
}
