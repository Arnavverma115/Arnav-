#include <stdio.h>
int main(void){
    int n=10, a[10], i,j, used;
    printf("Enter 10 integers:\n");
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    for(i=0;i<n;i++){
        used = 0;
        for(j=0;j<i;j++) if(a[j]==a[i]) { used = 1; break; }
        if(used) continue;
        int cnt = 0;
        for(j=0;j<n;j++) if(a[j]==a[i]) cnt++;
        printf("%d occurs %d times\n", a[i], cnt);
    }
    return 0;
}
